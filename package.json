{
  "name": "electron-react-boilerplate",
  "productName": "boilerplate",
  "version": "0.1.1",
  "description": "A React App with Tailwinds",
  "private": true,
  "author": {
    "name": "Ryan Collins",
    "email": "hello@ryd3v.com"
  },
  "copyright": "Copyright Â© 2023 Ryan Collins",
  "license": "ISC",
  "dependencies": {
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "got": ">=14.4.3",
    "minimatch": "^10.0.1",
    "nth-check": ">=2.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^4.2.3"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "yarn start",
    "electron": "electron .",
    "build:windows": "electron-builder --win --dir",
    "dist:windows": "electron-builder --win",
    "build:macos": "electron-builder --macos --dir",
    "dist:macos": "electron-builder --macos",
    "build:linux": "electron-builder --linux --dir",
    "dist:linux": "electron-builder --linux"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "electron": "^32.2.0",
    "electron-builder": "^25.1.8",
    "electron-compile": "^6.4.4",
    "got": ">=14.4.3",
    "loader-utils": "^3.3.1",
    "nth-check": ">=2.1.1",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "yargs-parser": "^21.1.1"
  },
  "build": {
    "productName": "boilerplate",
    "appId": "com.ryd3v.id",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "linux": {
      "category": "Utility"
    }
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
